{
  "name": "vscode-log",
  "displayName": "vscode-log",
  "version": "0.0.1",
  "packageManager": "pnpm@7.0.1",
  "description": "Visual Studio Code extension to outputting of log statements in various language files",
  "publisher": "odinlin",
  "icon": "assets/logo.png",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/topazur/vscode-log"
  },
  "engines": {
    "vscode": "^1.62.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:extension.insertLogStatement"
  ],
  "main": "./out/extension",
  "contributes": {
    "commands": [
      {
        "command": "extension.insertLogStatement",
        "title": "Insert Log Statement"
      }
    ],
    "keybindings": [
      {
        "command": "extension.insertLogStatement",
        "key": "shift+ctrl+l",
        "mac": "shift+cmd+l",
        "when": "editorTextFocus"
      }
    ],
    "configuration": {
      "type": "object",
      "title": "log statements extension configuration",
      "properties": {
        "vscode-log.options": {
          "type": "object",
          "default": {},
          "description": "log statements in various language files"
        }
      }
    }
  },
  "scripts": {
    "build": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "vscode:prepublish": "npm run build",
    "login": "vsce login",
    "package": "vsce package --no-dependencies",
    "publish": "vsce publish --no-dependencies",
    "release": "bumpp && npm run publish",
    "lint": "eslint ."
  },
  "devDependencies": {
    "@antfu/eslint-config-ts": "^0.23.0",
    "@types/node": "^17.0.31",
    "@types/vscode": "^1.62.0",
    "@vscode-use/utils": "^0.0.35",
    "@vscode/vsce": "^2.20.1",
    "bumpp": "^9.1.0",
    "eslint": "^8.15.0",
    "typescript": "^4.6.4"
  },
  "eslintIgnore": [
    "node_modules",
    "out",
    "template"
  ]
}
